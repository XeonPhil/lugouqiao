page{  
  display: flex;  
  flex-direction: column;  
  height: 100%;  
  background-color:#efeff4;
} 
.navbar{  
  flex: none;  
  display: flex;  
  background:#fff;  
}  
.navbar .item{  
  position: relative;  
  flex: auto;  
  text-align: center;  
  line-height: 80rpx;  
}  
.navbar .item.active{  
  color:green;  
}  
.navbar .item.active:after{  
  content: "";  
  display: block;  
  position: absolute;  
  bottom: 0;  
  left: 0;  
  right: 0;  
  height: 4rpx;  
  background: green;  
}  
.duiqi{
  display:flex;
}
.jiaobiao{
  width: 50%;
 
}
.liuyan{
  background: white;
  border-bottom: 1px solid #efeff4;
  line-height: 25px;
  padding-left: 10px;
  padding-bottom: 10px;
}